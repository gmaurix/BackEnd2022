 <div class="table-responsive">
        <table class="table table-striped table-sm rounded-2 responsive justify-center  mt-5 ">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
            </tr>
            {{#if existe}}
            {{#each productos}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.price}}</td>
                <td><img width="50px" height="50px" src='{{this.thumbnail}}' alt="foto producto"></td>
            </tr>
            {{/each}}
        </table>
          {{else}}  
          <h2 >no se encontraron productos</h2>
          {{/if}}
        <a href="/"class="btn btn-primary">Carga nuevo producto</a>
    </div>